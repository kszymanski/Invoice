<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<navigation-rule>
		<navigation-case>
			<from-outcome>goHome</from-outcome>
			<to-view-id>/faces/pages/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<converter>
    	<description>A Converter for RoleBean</description>
    	<converter-id>RoleBeanConverter</converter-id>
    	<converter-class>
        com.invoice.control.RoleConverter
    	</converter-class>
	</converter>
</faces-config>
